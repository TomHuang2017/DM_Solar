    <quoteString>N</quoteString>
    <sql>set @range_where_query='';
set @select_query='';
set @incremental_where_query='';
set @whole_select_where_query='';

select @range_where_query:= description from log_details where summary_id=${summary_id} and sub_module_name='range where query';
select @select_query:= description from log_details where summary_id=${summary_id} and sub_module_name='select query';
select @incremental_where_query:= description from log_details where summary_id=${summary_id} and sub_module_name='incremental where query';

set @range_where_query=replace(@range_where_query,'(begin_date_while)','${begin_date_while}');
set @range_where_query=replace(@range_where_query,'(month_per_while)','${month_per_while}');


SELECT @whole_select_where_query:=concat(@select_query,CASE WHEN 2=2 THEN @range_where_query else @incremental_where_query end)
from mysql.db limit 1;


SET @return_pre_details_id=0;
SELECT @return_pre_details_id:=MAX(details_id) from log_details where summary_id=${summary_id};
CALL log_details_add(${summary_id},'whole select where query',0,@whole_select_where_query,@return_pre_details_id);
select * from mysql.db limit 2;</sql>
