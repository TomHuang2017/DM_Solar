    <sql>declare @sql nvarchar(max)=''
set @sql='
select 
	ID,
	VisitNumber,
	EmpCode,
	RegisterDate,
	GETDATE() AS LastUpdateTime,
	IsDeleted
from FACT_OPD_Register_Demo
WHERE 1=1
'
set @sql=@sql+ 
	CASE WHEN '${etl_type}'='2' then 
		' AND IsDeleted =0 and RegisterDate>=cast(''${begin_date_while}'' as date) AND RegisterDate&lt;DATEADD(MONTH,${month_per_while},cast(''${begin_date_while}'' as date))'
		 ELSE ' AND LastUpdateTime>''${max_last_update_time}'''
	END

exec (@sql)
</sql>
